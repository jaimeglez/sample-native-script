<Page xmlns="http://schemas.nativescript.org/tns.xsd" loaded="pageLoaded">
  <scroll-view>
    <DockLayout stretchLastChild="true">
      <ListView items="{{ unitList }}" swipe="goToLogin">
        <ListView.itemTemplate>
          <Label text="{{ name }}" textWrap="true" class="title" tap="goToUnit"/>
        </ListView.itemTemplate>
      </ListView>
    </DockLayout>
  </scroll-view>
</Page>
